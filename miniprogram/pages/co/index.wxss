/* pages/co/index.wxss */
.page-section-spacing{
  margin-top: 60rpx;
}
.scroll-view_H{
  white-space: nowrap;
}
.scroll-view-item{
  height: 300rpx;
}
.scroll-view-item_H{
  display: inline-block;
  width: 100%;
  height: 300rpx;
}
.left-x{
  overflow-wrap: anywhere;
  display: flex;
  font-size: 29rpx;
  text-align: center;
  margin-bottom: 15rpx;
  height: 80rpx;
}

.left-x-x {
  width: 15%;
  margin: auto;
}

.left-x-code {
  width: 40%;
  margin: auto;
}

.left-x-name {
  width: 45%;
  margin: auto;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.b-all{
  display: flex;
}
right-x{
  width: 50%;
  font-size: 29rpx;
  text-align: center;
  
}
.right-x-x{
  display: inline-block;
  line-height: 80rpx;

}
.top-main{
  margin: auto;
  width: 70%;
  margin-bottom: 50rpx;
}

