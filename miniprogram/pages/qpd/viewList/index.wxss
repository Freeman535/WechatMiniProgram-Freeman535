.bgc{
  background: yellow;
  color: rgb(47, 50, 224);


}

.test{
  width: 100%;
  height: 20px;
  position: absolute;
  left: 0;
  display: flex;
  bottom: 0;
  margin: auto;
  text-align: center;

}
.clear{
  clear: both;
  overflow: hidden;
}
navigator{
  display:inline;
}
.list{
  margin-top:10px;
}
 
.list .list_item{
  margin-top:10px;
  padding:10px;
  height:100px;
  border-bottom:1px solid #E8E8E8;
}
.list .list_item .img{
  float:left;
  width:40%;
  height:100%;
  position: relative;
}
.list .list_item .img image{
   width:100%;
   height:100%;
}
 
.list .list_item .info{
  width:59%;
  float:right;
  height:100px;
  position:relative;
}
.list .list_item .info .title{
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  color:#333;
  margin-left:10px;
  font-size: 12px;
}
 
.list .list_item .info .price{
  color:#000000;
  margin-left:10px;
  margin-top:3px;
  font-size:12px;
  margin-bottom: 3px;
}
 
.list .list_item .info .num{
 
  left:0px;
  bottom:10px;
  color:#747474;
  margin-left:10px;
  font-size:12px;
}

.list .list_item .info .num2{

  left:0px;
  bottom:10px;
  color:#747474;
  margin-left:10px;
  font-size:12px;
}