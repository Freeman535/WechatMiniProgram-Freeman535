<!--components/search_1/search_1.wxml-->
<view class="input-list">
  <!--门店选择器-->
  <view class="input-list-main">
    <view></view>
    <picker data-id="0" bindchange="bindChange" value="{{index}}" range="{{fdarray['FDMC']}}">
      <view class="picker">
         门店当前选择：{{fdarray[showText[0]]['FDMC']}}
      </view>
    </picker>
  </view>

  <!--开始日期选择器-->
  <view wx:if="useS_RQ==true" class="input-list-main">
  <view></view>
  <picker data-id="1" mode="date" value="{{s_rqValue}}" start="2019-01-01" end="2025-09-01" bindchange="bindChange">
    <view class="picker">
      开始日期: {{showText[1]}}
    </view>
  </picker>
</view>

  <!--结束日期选择器-->
  <view class="input-list-main">
  <view></view>
  <picker data-id="2" mode="date" value="{{e_rqValue}}" start="2019-01-01" end="2025-09-01" bindchange="bindChange">
    <view class="picker">
      结束日期: {{showText[2]}}
    </view>
  </picker>
  </view>

  <!--单据编号-->
  <view class="input-list-main">
    <input data-id="3" bindinput="bindChange" value="{{showText[3]}}" type="number" placeholder="请输入单号" />
  </view>

  <!--商品条码-->
  <view class="input-list-main">
    <input data-id="4" bindinput="bindChange" value="{{showText[4]}}" type="number" placeholder="请输入条码" />
  </view>

  <!--商品代码-->
  <view class="input-list-main">
    <input data-id="5" bindinput="bindChange" value="{{showText[5]}}" type="number" placeholder="请输入编码" />
  </view>

  <!--商品名称-->
  <view class="input-list-main">
    <input data-id="6" bindinput="bindChange" value="{{showText[6]}}" type="text" placeholder="请输入名称" />
  </view>

  <button bindtap="bindSearch" type="primary" plain="true">查询</button>

</view>

<view>
  <view wx:if="{{canSearch==1}}">
    <!--在结果中查询-->
    <input bindinput="bindinput" type="text" placeholder="请输入条码或名称"/>
    <button bindtap="search_in" type="default" ></button>

    <view wx:if="{{back==4}}"> 
      <view wx:for="{{back_list_s}}" wx:key="key">
      <view>
        {{item.code}}
      </view>
      <view>
        {{item.name}}
      </view>
      <view>
        {{item.number}}
      </view>
      <view>
        {{item.yye}}
      </view>
      <view>
        {{item.zkje}}
      </view>
      <view>
        {{item.date}}
      </view>
      </view>
    </view>
    <view wx:elif="{{back==5}}">
      <view wx:for="{{back_list_s}}">
      <view>
        {{item.riqi}}
      </view>
      <view>
        {{item.code}}
      </view>
      <view>
        {{item.name}}
      </view>
      <view>
        {{item.number}}
      </view>
      <view>
        {{item.yye}}
      </view>
      <view>
        {{item.hscb}}
      </view>
      <view>
        {{item.zkje}}
      </view>
      <view>
        {{item.fml}}
      </view>
      </view>
    </view>
    <view wx:elif="{{back==6}}">
      <view wx:for="{{back_list_s}}">
        <view>
        {{item.code69}}
      </view>
      <view>
        {{item.code}}
      </view>
      <view>
        {{item.name}}
      </view>
      <view>
        {{item.jcsl}}
      </view>
      <view>
        {{item.je}}
      </view>
      <view>
        {{item.hsfs}}
      </view>
      </view>
    </view>
  </view>
</view>

